### Existing ontologies related to RDF data assessment and FAIR principles
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix ldqd: <https://www.w3.org/2016/05/ldqd> .
@prefix ldq: <http://linkeddata3.dia.fi.upm.es/ldq/#> .

### Newly-defined vocabularies speficif for the research project: "Interoperable Dimension for RDF Data"
@prefix iqd:<http://purl.org/iqd/> .
@prefix ip: <http://purl.org/ip/> .


############################
### Ontology Information ###
############################

iqd:
	a owl:Ontology ;
    dct:title "Interoperable Quality Dimension (iqm)"@en ;
    dct:description "The Interoperable Quality Dimension (iqd) vocabulary describes a set of quality dimensions which affects the interoperability of RDF data"@en ;
    dct:creator [
    		foaf:name "Shuxin Zhang";
    		foaf:mbox <mailto:s.x.zhang@amsterdamumc.nl>
    	] ;
    dct:modified "2020-10-05"^^xsd:date ;
    dct:language "en"^^xsd:language ;
	foaf:homepage <http://purl.org/iqd> .


##############################
### Interoperable Dimension###
##############################

#1. Semantic Accuracy
iqd:semanticAccuracy
    a dqv:Dimension;
    skos:broadMatch ldqd:semanticAccuracy;
    skos:prefLabel "Semantic accuracy"@en ;
	skos:definition "The data model should 1)be able to capture sufficient information or 2)represent the real object by correct labelling." @en ;
    skos:mappingRelation ip:good-data-model, ip:contextual-knowledge-of-links .


#2. Consistency
iqd:consistency
    a dqv:Dimension;
    skos:broadMatch ldqd:consistency;
    skos:prefLabel "Consistency"@en ;
	skos:definition "The axioms that describe digital objects (data model, vocabulary, and data) should be free of contradictions" @en ;
    skos:mappingRelation ip:good-data-model, ip:controlled-vocabulary, ip:properly-linked .

#3. Availability
iqd:availability
    a dqv:Dimension;
    skos:broadMatch ldqd:availability;
    skos:prefLabel "Availability"@en ;
	skos:definition "The extent to which vocabularies or data(or some portion of it) is present, obtainable and ready for use." @en ;
    skos:mappingRelation ip:globally-resolvable-vocabulary, ip:properly-linked .

#4.Interlinking
iqd:interlinking
    a dqv:Dimension;
    skos:broadMatch ldqd:interlinking;
    skos:prefLabel "Interlinking"@en ;
	skos:definition "Interlinking refers to the degree to which entities that represent the same concept are linked to each other, be it within or between two or more data sources."@en;
    skos:mappingRelation ip:properly-linked .

#5.Understandability
iqd:understandability
    a dqv:Dimension;
    skos:broadMatch ldqd:understandability;
    skos:prefLabel "Understandability"@en ;
	skos:definition "The axioms that describe digital objects (data model, vocabulary, and data) should be free of contradictions" @en ;
    skos:mappingRelation ip:good-data-model, ip:contextual-knowledge-of-links .

#6.Relevancy
iqd:relevancy
    a dqv:Dimension;
    skos:broadMatch ldqd:relevancy;
    skos:prefLabel "Relevancy"@en ;
	skos:definition "The provision of information which is in accordance with the task at hand and important to the usersâ€™ query."@en ;
    skos:mappingRelation ip:contextual-knowledge-of-links .

#7.Versatility
iqd:versatility
    a dqv:Dimension;
    skos:broadMatch ldqd:versatility;
    skos:prefLabel "Versatility"@en ;
	skos:definition "The availability of the data in different representations and in an internationalized way "@en ;
    skos:mappingRelation ip:properly-linked .

#8.Conciseness
iqd:conciseness
    a dqv:Dimension;
    skos:broadMatch ldqd:conciseness;
    skos:prefLabel "Conciseness"@en ;
	skos:definition "The minimization of redundancy of entities at the schema and the data level. "@en ;
    skos:mappingRelation ip:good-data-model, ip:properly-linked .

#9.Interpretability
iqd:interpretability
    a dqv:Dimension;
    skos:broadMatch ldqd:interpretability;
    skos:prefLabel "Interpretability"@en ;
	skos:definition "The data model should employ various schema languages (e.g., domain-specific ontologies) to provide definitions for terms. "@en ;
    skos:mappingRelation ip:good-data-model .

#10.Representational-consistency
iqd:representationalConsistency
    a dqv:Dimension;
    skos:broadMatch ldqd:interoperability;
    skos:prefLabel "Representational Consistency"@en ;
	skos:definition "The degree to which the format and structure of the information conforms to previously returned information as well as data from other sources. "@en ;
    skos:mappingRelation ip:good-data-model .

#11.Trustworthiness
iqd:trustworthiness
    a dqv:Dimension;
    skos:broadMatch ldqd:trustworthiness;
    skos:prefLabel "Trustworthiness"@en ;
	skos:definition "The degree to which the information is accepted to be correct, true, real and credible "@en ;
    skos:mappingRelation ip:commonly-used-vocabulary, ip:properly-linked .